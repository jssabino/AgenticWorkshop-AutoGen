{
  "studio_preferences": {
    "theme": "dark",
    "auto_save": true,
    "auto_save_interval": 30,
    "show_agent_avatars": true,
    "enable_sound_notifications": false,
    "default_workflow_view": "visual",
    "conversation_history_limit": 100
  },
  "ui_settings": {
    "sidebar_width": 300,
    "chat_panel_width": 400,
    "code_editor_theme": "vs-dark",
    "font_size": 14,
    "show_line_numbers": true,
    "word_wrap": true
  },
  "development_settings": {
    "debug_mode": false,
    "verbose_logging": false,
    "show_internal_messages": false,
    "enable_performance_monitoring": true
  },
  "export_settings": {
    "default_format": "json",
    "include_metadata": true,
    "include_conversation_history": true,
    "compress_exports": false
  },
  "security_settings": {
    "mask_api_keys_in_logs": true,
    "require_confirmation_for_deletions": true,
    "enable_backup_before_changes": true,
    "session_timeout_minutes": 120
  }
}
