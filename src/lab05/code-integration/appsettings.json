{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MCP": {
    "ServerUrl": "http://localhost:3000",
    "Timeout": 30,
    "RetryAttempts": 3
  },
  "GitHub": {
    "DefaultRepository": "microsoft/autogen",
    "MaxIssuesPerRequest": 10,
    "MaxCommitsPerRequest": 20
  },
  "OpenAI": {
    "Model": "gpt-4",
    "Temperature": 0.7,
    "MaxTokens": 2000
  }
}
